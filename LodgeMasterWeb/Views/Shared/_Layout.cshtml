<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" href="~/assets/images/main-logo.png" type="image/x-icon">
    <link rel="icon" href="~/assets/images/main-logo.png" type="image/x-icon">
    <title>LodgesMaster - @ViewBag.Title</title>
    <link rel="stylesheet" href="~/assets/css/fontawesome.min.css">
    <link rel="stylesheet" href="~/lib/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/assets/css/style.css" asp-append-version="true">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
</head>
<body>
    @RenderBody()

    <script src="~/assets/js/jquery-3.6.3.min.js"> </script>
    <script src="~/lib/bootstrap/js/bootstrap.bundle.min.js"> </script>
    <script src="~/assets/js/popper.min.js"> </script>
    <script src="~/assets/js/main.js" asp-append-version="true"> </script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
