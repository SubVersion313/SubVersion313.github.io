         function itemNotes () {

            $.ajax({
                url: '@Url.Action("GetItemsQty", "Dashboard")', // رابط الـ API الخاص بالباكند
                type: "GET",
                dataType: "json",
                cache: false,
                success: function (result) {
                    if (result.success) {
                        // تحقق إذا كانت البيانات متوفرة
                        if (result.data && result.data.length > 0) {
                            // بناء الجدول
                            let tableBody = "";
                            $.each(result.data, function (index, order) {
                                tableBody += `
                                    <tr>
                                        <td>${order.OrderId}</td>
                                        <td>${order.OrderDate}</td>
                                        <td>${order.CustomerName}</td>
                                        <td>${order.TotalAmount}</td>
                                        <td>${order.Status}</td>
                                    </tr>
                                `;
                            });

                            // إدخال البيانات في الجدول
                            $("#itemOrdersTable tbody").html(tableBody);
                        } else {
                            // إذا لم تكن هناك بيانات
                            $("#itemOrdersTable tbody").html('<tr><td colspan="5">لا توجد طلبات لعرضها</td></tr>');
                        }
                    } else {
                        alert("فشل في جلب البيانات!");
                    }
                },
                error: function (xhr, status, error) {
                    console.error("خطأ: " + error);
                    alert("حدث خطأ أثناء جلب البيانات!");
                }
            });
        }

